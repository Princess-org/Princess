import common

def #test test_arithmethic {
    assert run_repl("10 + 20") == "30"
    assert run_repl("10 * 20") == "200"
}

def #test test_func_call {
    assert run_repl("cstd::printf(\"%d\".value, 10)") == "102"
    assert run_repl("print(10)") == "102"
}

def #test test_reflection {
    assert run_repl("int.size") == "4"
}